        15,
        16,
        17,
        "next"
      ],
      "safari": [
        4,
        5.1,
        6
      ]
    },
    "files": [
      "contra.js",
      "contra.shim.js",
      "test/*.js"
    ],
    "harness": "mocha"
  },
  "devDependencies": {
    "assert": "~1.1.0",
    "browserify": "10.2.4",
    "jshint": "~2.4.1",
    "jshint-stylish": "~0.1.5",
    "jshint-tap": "0.0.1",
    "mocha": "~1.17.0",
